-- DbDiff --
<script type="text/javascript" charset="utf-8" src="http://api.caffeina.mx/jquery/jquery-1.4.2.min.js"></script>

<script>
	var oldResults = null,
		newResults = null;
	
	function check( cb ){
		$.ajax({
		  url: '/',
		  dataType : "json",
		  success: function(a){
			if(a.success === undefined)
			return cb.call(null, a);
			
			$("#status").html(a.reason);
		  },
		  data :{
			action:"AdminPAQProxy",
			dbdiff:"1",
			path: $("#path").val()
		}
		});
	}
	
	function boot(){
		check( function( data ){
			oldResults = data;
		});
	}
	
	
	function compare(){
		if(null === oldResults) {
			$("#status").html("OK! Now do your action and click check again.");
			return boot();	
		}
		
		
		check(function(data){
			newResults = data;
			
			if(newResults.length != oldResults.length){
				$("#status").html("<b>file count mismatch: something is wrong.</b>");
				return;
			}
			
			var changedHtml = "",
				changedCount = 0;

			
			for (var i=0; i < newResults.length; i++) {
				
				if(newResults[i].file 
					!= oldResults[i].file 
				){
					$("#status").html("<b>file order mismatch: something is wrong.</b>");
					return;
				}
					
				

				if( newResults[i].md5
					!== oldResults[i].md5
				){
					changedHtml += "<tr><td>" + newResults[i].file + "</td></tr>";
					changedCount ++;
				}
				
				
			};
			
			oldResults = newResults;
			
			if( changedCount == 0 ){
				$("#status").html("No changes were made to the files.");
			}else{
				
				$("#status").html("<table>" 
									+ changedCount + " file(s) changed.</tr>"
									+ changedHtml + "</table>");				
			}

		});
	}
	
	
	
</script>
<html>
	<div id="status">
		Click check!
	</div>
	<input type="text"  placeholder="path to adminpaq files" id="path">
	<input type="button" value="test" onClick="compare()">
</html>
-- /DbDiff --





